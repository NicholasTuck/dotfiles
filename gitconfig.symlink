	email = nick@nicholastuck.com
[core]
	editor = vim
	pager = 
	autocrlf = true
[help]
	autocorrect = 1
[color]
	ui = true
[push]
	default = simple
[alias]
	st = status -s
	forget = rm --cached 
	glog = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
